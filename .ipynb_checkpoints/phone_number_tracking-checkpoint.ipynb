   "outputs": [],
   "source": [
    "import phonenumbers \n",
    "from phonenumbers import geocoder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "a = input(\"Enter the Phone Number: \")\n",
    "phonenumber = phonenumbers.parse(a)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#display the location\n",
    "print(geocoder.description_for_number(phonenumber,'en'))"
   ]
